{
  "name": "savvy-dashboard",
  "version": "1.0.0",
  "description": "Savvy Funnel Analytics Dashboard",
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 prisma generate && node --max-old-space-size=8192 ./node_modules/next/dist/bin/next build",
    "start": "next start",
    "lint": "next lint",
    "test": "node test-connection.js",
    "test:query": "node scripts/test-query.js",
    "test:dashboard": "node scripts/test-dashboard-queries.js",
    "verify:game": "node scripts/verify-pipeline-catcher.js",
    "verify:game:api": "node scripts/verify-pipeline-catcher.js --api",
    "verify:recruiter-security": "node scripts/verify-recruiter-security.js",
    "postinstall": "prisma generate",
    "gen:api-routes": "node scripts/generate-api-inventory.cjs",
    "gen:models": "node scripts/generate-model-inventory.cjs",
    "gen:env": "node scripts/generate-env-inventory.cjs",
    "gen:all": "node scripts/generate-api-inventory.cjs && node scripts/generate-model-inventory.cjs && node scripts/generate-env-inventory.cjs",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@auth/prisma-adapter": "^2.11.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@google-cloud/bigquery": "^7.9.4",
    "@google-cloud/bigquery-data-transfer": "^5.1.2",
    "@prisma/client": "6.19.0",
    "@sendgrid/mail": "^8.1.6",
    "@sentry/nextjs": "^10.34.0",
    "@tremor/react": "^3.18.7",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.34.3",
    "@vercel/analytics": "^1.6.1",
    "@vercel/postgres": "^0.10.0",
    "bcryptjs": "^3.0.3",
    "date-fns": "^3.6.0",
    "dotenv": "^16.3.1",
    "googleapis": "^170.0.0",
    "html-to-image": "^1.11.13",
    "jsonwebtoken": "^9.0.3",
    "jszip": "^3.10.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.300.0",
    "next": "^14.2.35",
    "next-auth": "^4.24.13",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "recharts": "^3.6.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20.19.28",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.23",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.35",
    "husky": "^9.1.7",
    "knip": "^5.84.1",
    "postcss": "^8.5.6",
    "prisma": "6.19.0",
    "tailwindcss": "^3.4.19",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
